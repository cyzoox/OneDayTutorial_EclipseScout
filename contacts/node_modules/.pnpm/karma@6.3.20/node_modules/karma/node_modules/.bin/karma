#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/clintjamesrpascual/IdeaProjects/contacts/contacts/node_modules/.pnpm/karma@6.3.20/node_modules/karma/bin/node_modules:/home/clintjamesrpascual/IdeaProjects/contacts/contacts/node_modules/.pnpm/karma@6.3.20/node_modules/karma/node_modules:/home/clintjamesrpascual/IdeaProjects/contacts/contacts/node_modules/.pnpm/karma@6.3.20/node_modules:/home/clintjamesrpascual/IdeaProjects/contacts/contacts/node_modules/.pnpm/node_modules:/home/clintjamesrpascual/IdeaProjects/contacts/contacts/node_modules:/home/clintjamesrpascual/IdeaProjects/contacts/node_modules:/home/clintjamesrpascual/IdeaProjects/node_modules:/home/clintjamesrpascual/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/clintjamesrpascual/IdeaProjects/contacts/contacts/node_modules/.pnpm/karma@6.3.20/node_modules/karma/bin/node_modules:/home/clintjamesrpascual/IdeaProjects/contacts/contacts/node_modules/.pnpm/karma@6.3.20/node_modules/karma/node_modules:/home/clintjamesrpascual/IdeaProjects/contacts/contacts/node_modules/.pnpm/karma@6.3.20/node_modules:/home/clintjamesrpascual/IdeaProjects/contacts/contacts/node_modules/.pnpm/node_modules:/home/clintjamesrpascual/IdeaProjects/contacts/contacts/node_modules:/home/clintjamesrpascual/IdeaProjects/contacts/node_modules:/home/clintjamesrpascual/IdeaProjects/node_modules:/home/clintjamesrpascual/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/karma" "$@"
else
  exec node  "$basedir/../../bin/karma" "$@"
fi
